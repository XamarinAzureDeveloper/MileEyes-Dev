<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MileEyes.CustomControls.OriginAddressViewCell">
  <StackLayout Orientation="Horizontal" Padding="15, 0, 15, 0">
    <Image Source="origin.png" Margin="0,0,7.5,0"/>
    <Label Text="Origin"
    TextColor="#FFFFFF" VerticalTextAlignment="Center" />

    <Label Text="{Binding OriginAddress.Label}"
    HorizontalOptions="EndAndExpand"
    TextColor="#55FFFFFF"  VerticalTextAlignment="Center" LineBreakMode="TailTruncation"/>
  </StackLayout>
</ViewCell>